{
  "name": "six-cities",
  "version": "4.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "start:dev": "node --loader ts-node/esm ./src/main.ts | pino-pretty --colorize --translateTime SYS:standard",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "mock:server": "json-server ./mocks/mock-server-data.json --port 3012"
  },
  "dependencies": {
    "@typegoose/typegoose": "11.1.0",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "chalk": "^5.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "dayjs": "^1.11.10",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "got": "12.5.3",
    "http-status-codes": "^2.2.0",
    "inversify": "6.0.1",
    "jose": "^4.11.1",
    "mime-types": "^2.1.35",
    "mongoose": "7.1.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.0",
    "pino": "8.14.1",
    "reflect-metadata": "0.1.13",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/convict": "6.1.1",
    "@types/convict-format-with-validator": "6.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.55.0",
    "eslint-config-htmlacademy": "9.1.1",
    "json-server": "^0.17.1",
    "pino-pretty": "10.0.0",
    "prettier": "^3.0.3",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  }
}
